      </div>

      <!-- BODY DE LA SECTION FORMULAIRE -->
      <div class="row">
        <div class="container contact-form">

          <form method="post">
            <h3 class="align-items-center">Laissez-nous un message</h3>
            <div class="row">
              <div class="col-lg-6">
                <div class="form-group">
                  <input type="text" name="textName" class="form-control" placeholder="Votre nom *" value="" />
                </div>
                <div class="form-group">
                  <input type="text" name="textEmail" class="form-control" placeholder="Votre email *" value="" />
                </div>
                <div class="form-group">
                  <input type="text" name="textPhone" class="form-control" placeholder="Votre numéro *" value="" />
                </div>
              </div>
              <div class="col-md-6">
                <div class="form-group">
                  <textarea name="textmessage" class="form-control" placeholder="Your Message *"
                    style="width: 100%; height: 150px;"></textarea>
                </div>
              </div>

            </div>
            <div class="form-group">
              <input type="submit" name="btnSubmit" class="btnContact" value="Envoyer votre demande" />
            </div>
          </form>
        </div>
      </div>
    </div>
  </section>
